<p id="notice"><%= notice %></p>

<h1>Outputs</h1>

<table>
  <thead>
    <tr>
      <th>Int</th>
      <th>Int</th>
      <th>String</th>
      <th>Text</th>
      <th>Float</th>
      <th>Tinyint</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outputs.each do |output| %>
      <tr>
        <td><%= output.int %></td>
        <td><%= output.int %></td>
        <td><%= output.string %></td>
        <td><%= output.text %></td>
        <td><%= output.float %></td>
        <td><%= output.tinyint %></td>
        <td><%= output.date %></td>
        <td><%= link_to 'Show', output %></td>
        <td><%= link_to 'Edit', edit_output_path(output) %></td>
        <td><%= link_to 'Destroy', output, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Output', new_output_path %>
